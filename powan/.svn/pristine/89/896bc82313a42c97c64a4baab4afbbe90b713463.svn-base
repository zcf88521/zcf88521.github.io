@import url(../../css/bootstrap.css);
@import url(../../css/normalize.css);
@import url(../../css/base.css);
.html, body, p {
  padding: 0;
  margin: 0;
}

body {
  min-width: 320px;
  background-color: #f5f9f3;
}

.cabbage_heard {
  width: 100%;
  height: 44px;
  background: #e4393c;
  text-align: center;
  line-height: 44px;
  font-size: 20px;
  font-weight: 800;
  color: white;
  padding-left: 20px;
  padding-right: 50px;
}
.cabbage_heard a {
  color: white;
  position: absolute;
  top: 11px;
}
.cabbage_heard a.cabbage_heard_left {
  font-size: 30px;
  left: 20px;
  top: 6px;
}
.cabbage_heard a.cabbage_heard_right {
  right: 10px;
  background-image: url("../../img/header_app.png");
  background-repeat: no-repeat;
  background-size: 32px 32px;
  width: 32px;
  height: 44px;
  top: 6px;
}

.cabbage_nav {
  background-color: white;
  width: 100%;
  height: 38px;
  border-bottom: 1px solid #efefef;
  position: relative;
  padding-right: 38px;
  overflow: hidden;
}
.cabbage_nav .cabbage_nav_all ul {
  width: 1000%;
  height: 38px;
  line-height: 38px;
}
.cabbage_nav .cabbage_nav_all ul li {
  float: left;
  padding: 0 13px;
}
.cabbage_nav .cabbage_nav_all ul li a {
  height: 36px;
  text-align: center;
  line-height: 36px;
  font-size: 14px;
  color: black;
  display: block;
}
.cabbage_nav span {
  position: absolute;
  right: 0;
  width: 38px;
  height: 38px;
  font-size: 25px;
  line-height: 38px;
  text-align: center;
  color: gray;
  border-left: 1px solid #efefef;
  background-color: white;
  z-index: 1;
}

.cabbage_img {
  text-align: center;
  margin-top: 10px;
  margin-bottom: 10px;
  width: 100%;
}
.cabbage_img img {
  width: 100%;
}

.cabbage_content {
  width: 100%;
  height: 152px;
  margin: 6px auto;
  background-color: #ffffff;
  position: relative;
  padding: 10px 5px;
  box-sizing: border-box;
  overflow: hidden;
  border-top: 1px solid #e1e1e1;
  border-bottom: 1px solid #e1e1e1;
}
.cabbage_content .media-object {
  border: 1px solid #e1e1e1;
  width: 130px;
  height: 130px;
}
.cabbage_content .media-body .media-heading {
  min-height: 28px;
  max-height: 40px;
}
.cabbage_content .media-body .content_pr {
  font-size: 24px;
  color: #fc353a;
  margin-top: 4px;
  position: relative;
  width: 100%;
  height: auto;
  overflow: visible;
}
.cabbage_content .media-body .content_pr i {
  font-size: 16px;
  vertical-align: middle;
  font-style: normal;
  margin-right: 4px;
}
.cabbage_content .media-body .content_pr em {
  font-size: 12px;
  vertical-align: middle;
  font-style: normal;
}
.cabbage_content .media-body .content_pr del {
  text-decoration: line-through;
  font-size: 13px;
  color: #a8a8a8;
  margin-left: 12px;
  vertical-align: middle;
}
.cabbage_content .media-body .content_link .link_quan {
  border: 1px dotted #e4393c;
  height: 26px;
  line-height: 26px;
  text-align: center;
  display: block;
  padding: 0px 5px;
  font-size: 12px;
  color: #e4393c;
}
.cabbage_content .media-body .content_link .link_dan {
  height: 38px;
  line-height: 38px;
  text-align: center;
  display: block;
  background-color: #ff5e5d;
  color: #ffffff;
  border-radius: 4px;
  padding: 0px 10px;
  font-size: 12px;
}

.cabbage_endback {
  width: 100%;
  margin-top: 20px;
  height: 38px;
  border-top: solid 1px #ebebeb;
  border-bottom: 1px solid #ccc;
  line-height: 36px;
  padding-left: 8px;
  background: gradient(linear, 0 0, 0 100%, from(#ffffff), to(#ebebeb));
}
.cabbage_endback a {
  font-size: 14px;
  color: black;
  font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif !important;
}
.cabbage_endback a:hover {
  text-decoration: none;
  color: black;
}

.cabbage_link {
  width: 100%;
  height: 100px;
  background-color: #434343;
  margin-top: 20px;
  padding: 10px;
  position: relative;
}
.cabbage_link .media-left a img {
  width: 64px;
  height: 64px;
}
.cabbage_link .media-body {
  color: white;
}
.cabbage_link .media-body a {
  display: block;
  width: 80px;
  height: 30px;
  line-height: 30px;
  font-size: 14px;
  color: #FFFFFF;
  background: #6db110;
  border-radius: 4px;
  text-align: center;
  position: absolute;
  top: 4px;
  right: 10px;
}

.cabbage_footer {
  background: #efefef;
  text-align: center;
  font-size: 12px;
  padding: 15px 0;
  color: #666;
  width: 100%;
  line-height: 10px;
  border-top: 1px solid #e1e1e1;
  border-bottom: 1px solid #e1e1e1;
  margin-top: 20px;
}
.cabbage_footer .foot_one {
  color: red;
}
.cabbage_footer .foot_two {
  color: #666;
}
