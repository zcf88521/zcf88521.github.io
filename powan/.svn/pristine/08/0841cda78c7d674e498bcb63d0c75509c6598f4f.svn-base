<!--JQ-->
<script src="js/jquery-1.12.2.js"></script>
<!--bootstarp-->
<script src="js/bootstrap.js"></script>
<!--模板引擎-->
<script src="js/template.js"></script>


<!--商品介绍/二维码/评论区模版-->
<script id="detail" type="text/html">
  <!--商品区-->
  <div class="produc">
    <!--标题-->
    <h3 class="title">{{productName}}</h3>
    <!--价格-->
    <h4 class="price">{{productPinkage}}</h4>
    <!--商品来源信息-->
    <div class="info"><span>{{productFrom}}</span><span>{{productTime}}</span><span>{{productTips}}</span><span><a href="javaScript:void(0)">{{productComCount}}</a></span></div>
    <!--商品介绍文本-->
    <div class="text">
      <div class="media">
        <div class="media-right f_r">
          <a href="#">{{#productImgSm}}</a>
        </div>
        <div class="media-body" style="display:inline">
          <h4 class="media-heading">
            {{productInfo2}}
          </h4>
        </div>

      </div>
      <p>{{productInfo1}}</p>
      <p>{{productInfo}}</p>
      <div class="imgs">
        {{#productImgLg}}
          {{#productImg2}}
            {{#productImg3}}
      </div>
      {{#productCity}}
    </div>
    <!--前往购买按钮-->
    <div class="goBuy">
      <a href="javaScript:void(0)">前往购买</a>
    </div>
  </div>
  <!--二维码-->
  <div class="qrCode">
    <img src="images/mmbweixin2.png" alt="">
  </div>
  <!--评论-->
  <div class="comments">
    {{#productComment}}
  </div>
</script>


<script src="js/ljj/overseas_detail.js"></script>
