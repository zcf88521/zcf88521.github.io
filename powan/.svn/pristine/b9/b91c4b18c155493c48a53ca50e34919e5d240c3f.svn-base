@import url(../../css/bootstrap.css);
@import url(../../css/normalize.css);
@import url(../../css/base.css);
.html, body, p {
  padding: 0;
  margin: 0;
}

body {
  min-width: 320px;
  background-color: #f5f9f3;
  -webkit-transition: none;
          transition: none;
}

.bt {
  border-bottom: 2px solid red;
}

.cabbage_heard {
  background: #f4483e;
  color: #FFFFFF;
  height: 44px;
  width: 100%;
  position: relative;
}
.cabbage_heard .cabbage_heard_left {
  position: absolute;
  width: 50px;
  height: 44px;
  text-align: center;
  left: 0;
}
.cabbage_heard h1 {
  display: block;
  margin: 0;
  text-align: center;
  line-height: 44px;
  font-size: 20px;
  font-weight: bold;
}
.cabbage_heard .cabbage_heard_right {
  position: absolute;
  width: 50px;
  height: 44px;
  line-height: 44px;
  right: 5px;
  top: 0px;
}
.cabbage_heard .cabbage_heard_right img {
  width: 80%;
  height: 80%;
}

.cabbage_nav {
  background-color: white;
  width: 100%;
  height: 38px;
  border-bottom: 1px solid #efefef;
  position: relative;
  padding-right: 38px;
  overflow: hidden;
}
.cabbage_nav .cabbage_nav_all ul {
  width: 150%;
  height: 38px;
  line-height: 38px;
}
.cabbage_nav .cabbage_nav_all ul li {
  float: left;
  padding: 0 13px;
}
.cabbage_nav .cabbage_nav_all ul li a {
  height: 36px;
  text-align: center;
  line-height: 36px;
  font-size: 14px;
  color: black;
  display: block;
  text-decoration: none;
}
.cabbage_nav span {
  position: absolute;
  right: 0;
  width: 38px;
  height: 38px;
  font-size: 25px;
  line-height: 38px;
  text-align: center;
  color: gray;
  border-left: 1px solid #efefef;
  background-color: white;
  z-index: 1;
}

.cabbage_img {
  text-align: center;
  margin-top: 10px;
  margin-bottom: 10px;
  width: 100%;
}
.cabbage_img img {
  width: 100%;
}

.cabbage_content {
  width: 100%;
  height: 100%;
  background-color: #ffffff;
  position: relative;
}
.cabbage_content li {
  padding: 10px 5px;
  box-sizing: border-box;
  margin: 6px auto;
  overflow: hidden;
  border-top: 1px solid #e1e1e1;
  border-bottom: 1px solid #e1e1e1;
}
.cabbage_content .media img {
  width: 130px;
  height: 130px;
}
.cabbage_content .media-body .media-heading {
  min-height: 28px;
  max-height: 40px;
  font-size: 14px;
}
.cabbage_content .media-body .content_pr {
  font-size: 24px;
  color: #fc353a;
  margin-top: 4px;
  width: 100%;
  height: auto;
  overflow: visible;
}
.cabbage_content .media-body .content_pr i {
  font-size: 16px;
  vertical-align: middle;
  font-style: normal;
  margin-right: 4px;
}
.cabbage_content .media-body .content_pr em {
  font-size: 12px;
  vertical-align: middle;
  font-style: normal;
}
.cabbage_content .media-body .content_pr del {
  text-decoration: line-through;
  font-size: 13px;
  color: #a8a8a8;
  margin-left: 12px;
  vertical-align: middle;
}
.cabbage_content .media-body .content_link .link_quan {
  border: 1px dotted #e4393c;
  height: 26px;
  line-height: 26px;
  text-align: center;
  display: block;
  padding: 0px 5px;
  font-size: 12px;
  color: #e4393c;
}
.cabbage_content .media-body .content_link .link_dan {
  height: 38px;
  line-height: 38px;
  text-align: center;
  display: block;
  background-color: #ff5e5d;
  color: #ffffff;
  border-radius: 4px;
  padding: 0px 10px;
  font-size: 12px;
}
.cabbage_content .media-body .bar b {
  font-weight: normal;
  font-size: 12px;
  color: #666;
  vertical-align: 3px;
}

.cabbage_endback {
  width: 100%;
  margin-top: 20px;
  height: 38px;
  border-top: solid 1px #ebebeb;
  border-bottom: 1px solid #ccc;
  line-height: 36px;
  padding-left: 8px;
  background: gradient(linear, 0 0, 0 100%, from(#ffffff), to(#ebebeb));
}
.cabbage_endback a {
  font-size: 14px;
  color: black;
  font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif !important;
}
.cabbage_endback a:hover {
  text-decoration: none;
  color: black;
}

.cabbage_link {
  width: 100%;
  height: 100px;
  background-color: #434343;
  margin-top: 20px;
  padding: 10px;
  position: relative;
}
.cabbage_link .media-left a img {
  width: 64px;
  height: 64px;
}
.cabbage_link .media-body {
  color: white;
}
.cabbage_link .media-body a {
  display: block;
  width: 80px;
  height: 30px;
  line-height: 30px;
  font-size: 14px;
  color: #FFFFFF;
  background: #6db110;
  border-radius: 4px;
  text-align: center;
  position: absolute;
  top: 4px;
  right: 10px;
}

.cabbage_footer {
  background: #efefef;
  text-align: center;
  font-size: 12px;
  padding: 15px 0;
  color: #666;
  width: 100%;
  line-height: 10px;
  border-top: 1px solid #e1e1e1;
  border-bottom: 1px solid #e1e1e1;
  margin-top: 20px;
}
.cabbage_footer .foot_one {
  color: red;
}
.cabbage_footer .foot_two {
  color: #666;
}

.cabbage_backtop a {
  position: fixed;
  z-index: 999;
  bottom: 30px;
  right: 8px;
  width: 40px;
  text-align: center;
  line-height: 40px;
  font-size: 30px;
  height: 40px;
  border-radius: 20px;
  opacity: 0.5;
}
