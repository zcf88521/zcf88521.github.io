header {
  height: 38px;
  background-color: #ebebeb;
  background: -webkit-gradient(linear, 0 0, 0 100%, from(#ffffff), to(#ebebeb));
  border-top: solid 1px #ebebeb;
  border-bottom: 1px solid #ccc;
  line-height: 36px;
  padding-left: 8px;
  font-size: 14px;
  color: #5a5a5a; }

.hd {
  height: 36px;
  background: #f3f3f3;
  border: 1px solid #c8c8c8;
  position: relative; }
  .hd h3 {
    position: absolute;
    top: -20px;
    left: 0;
    list-style: none;
    list-style: none;
    height: 36px;
    line-height: 36px;
    font-size: 14px;
    padding-left: 10px;
    color: #874604;
    font-weight: bold; }

ul li {
  height: 57px; }
  ul li a {
    display: block;
    height: 36px;
    width: 100%;
    padding-left: 30px;
    position: relative;
    text-decoration: none; }
    ul li a em {
      display: block;
      width: 20px;
      height: 20px;
      position: absolute;
      text-align: center;
      line-height: 20px;
      background-color: #ccc;
      left: 5px;
      top: 20px; }
    ul li a .inner {
      width: 100%;
      height: 36px; }
      ul li a .inner .tit {
        font-size: 18px;
        color: #000000; }
      ul li a .inner p {
        font-size: 14px;
        color: #999999;
        margin-top: 3px; }

.sellList li {
  height: 120px; }
  .sellList li a {
    text-decoration: none;
    padding-left: 100px;
    display: block;
    width: 100%;
    height: 107px;
    position: relative; }
    .sellList li a .list_left {
      width: 100px;
      height: 100px;
      position: absolute;
      left: 0; }
      .sellList li a .list_left img {
        width: 100%; }
    .sellList li a .list_right {
      width: 100%; }
      .sellList li a .list_right .top {
        min-height: 45px; }
      .sellList li a .list_right .center {
        height: 25px; }
        .sellList li a .list_right .center .star {
          float: right; }
      .sellList li a .list_right .bottom {
        height: 24px; }

.pllist li a {
  text-decoration: none;
  display: block;
  width: 100%;
  height: 107px;
  position: relative; }
  .pllist li a .list_right {
    width: 100%; }
    .pllist li a .list_right .top {
      min-height: 45px; }
