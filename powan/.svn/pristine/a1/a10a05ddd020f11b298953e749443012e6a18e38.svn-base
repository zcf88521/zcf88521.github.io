@charset "UTF-8";
body {
  background: #f3f3f3;
  min-width: 320px;
}

header {
  height: 51px;
  background-color: #ff841d;
  border-bottom: 1px solid #e6e6e6;
}
header a {
  display: block;
  height: 100%;
}
header a img {
  overflow: hidden;
}
header .header_left {
  width: 75%;
  margin-left: 5px;
  padding: 12px;
}
header .header_left img {
  max-height: 25px;
}
header .header_right {
  padding: 7px;
}
header .header_right img {
  max-height: 34px;
}
header .header_hidden {
  background: #EFEFEF;
  border-bottom: 1px solid #ddd;
  height: 44px;
  position: fixed;
  display: none;
}
header .header_hidden .btn_serach {
  height: 44px;
  width: 44px;
  overflow: hidden;
  position: relative;
  color: transparent;
}

.goods_serach {
  border-bottom: 1px solid #e6e6e6;
  padding: 10px;
  position: relative;
  background-color: White;
}
.goods_serach input {
  font-size: 16px;
  height: 30px;
  line-height: 30px;
  border-radius: 15px;
}
.goods_serach .serachText {
  background: #f1f1f1;
  padding-left: 10px;
  width: 90%;
  padding-left: 5%;
  border: none;
}
.goods_serach .serachBtn {
  width: 63px;
  background: #ff841d;
  color: #FFFFFF;
  position: absolute;
  top: 10px;
  right: 10px;
}

.appris_nav {
  height: 36px;
  background-color: #ebebeb;
  border-top: 1px solid #ebebeb;
  border-bottom: 1px solid #ebebeb;
  background: -webkit-linear-gradient(top, #fff, #ebebeb);
  background: linear-gradient(top, #fff, #ebebeb);
  text-align: left;
  line-height: 36px;
  font-size: .7em;
}
.appris_nav span:nth-of-type(1) {
  color: #666;
  padding-left: 5px;
}

.appris_mian .hd {
  background: #f3f3f3;
  color: #874604;
}
.appris_mian .hd h3 {
  line-height: 36px;
  border-top: 1px solid #c8c8c8;
  border-bottom: 1px solid #c8c8c8;
  font-size: .6em;
  font-weight: 600;
  padding-left: 5px;
}
.appris_mian .body ul {
  padding-bottom: 30px;
}
.appris_mian .body ul li {
  padding: 10px;
  border-bottom: 1px solid #e7e7e7;
}
.appris_mian .body ul li a {
  display: block;
}
.appris_mian .body ul li a .protit_img img {
  width: 80px;
  height: 80px;
}
.appris_mian .body ul li a .tit {
  font-size: .5em;
  overflow: hidden;
  padding-left: 10px;
  padding-top: 5px;
  color: #888;
}
.appris_mian .body ul li .pro_appris {
  background: #f8f8f8;
  margin-top: 10px;
  font-size: .7em;
  padding: 10px;
  border: 1px solid #e7e7e7;
  position: relative;
}
.appris_mian .body ul li .pro_appris .arrow {
  width: 0;
  height: 0;
  position: absolute;
  display: block;
  left: 29px;
  top: -8px;
  border-bottom: 8px solid #f8f8f8;
  border-left: 8px solid transparent;
  border-right: 8px solid transparent;
}
.appris_mian .body ul li .pro_appris .pro_app_hd {
  height: 20px;
  line-height: 20px;
}
.appris_mian .body ul li .pro_appris .star5 {
  display: block;
  height: 18px;
  width: 90px;
  background: url(../../images/star.svg) repeat-x center/18px 18px;
}
.appris_mian .body ul li .pro_appris p {
  padding-top: 10px;
  background: #f8f8f8;
}

/*底部区域*/
footer .footer_top {
  background: #efefef;
  border-top: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
  height: 34px;
  line-height: 34px;
}
footer .footer_top .footer_log {
  width: 30%;
}
footer .footer_top .footer_back {
  width: 40%;
  border: 0;
}
footer .footer_top a {
  display: block;
  height: 100%;
  text-align: center;
  box-sizing: border-box;
  float: left;
  font-size: 14px;
  border-right: 1px solid #c4c4c4;
}
footer .footer_top a img {
  width: 15px;
  height: 15px;
  vertical-align: middle;
  margin-top: -5px;
  margin-right: 2px;
}
footer .footer_bottom {
  text-align: center;
  background: #efefef;
  font-size: 12px;
  padding: 15px 0;
  color: #666;
}
footer .footer_bottom p a:first-child {
  color: red;
}
