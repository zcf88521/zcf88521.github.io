html,
body {
  width: 100%;
  min-width: 320px;
  background-color: #F7F7F7;
}

a {
  text-decoration: none;
}

a:focus {
  text-decoration: none;
}

header {
  height: 51px;
  background-color: #ff841d;
  border-bottom: 1px solid #e6e6e6;
}
header a {
  display: block;
  height: 100%;
}
header a img {
  overflow: hidden;
}
header .header_left {
  width: 75%;
  margin-left: 5px;
  padding: 12px;
}
header .header_left img {
  max-height: 25px;
}
header .header_right {
  padding: 7px;
}
header .header_right img {
  max-height: 34px;
}
header .header_hidden {
  background: rgba(255, 255, 255, 0.9);
  border-bottom: 1px solid #ddd;
  height: 44px;
  width: 100%;
  position: fixed;
  z-index: 77;
  display: none;
}
header .header_hidden .btn_serach {
  height: 44px;
  width: 44px;
  overflow: hidden;
  color: transparent;
  position: absolute;
  top: 0;
}
header .header_hidden .btn_serach::after {
  content: '';
  position: absolute;
  top: 0;
  display: block;
  width: 100%;
  height: 44px;
  background-image: url(../../images/icon_bar.png);
  background-repeat: no-repeat;
  background-size: 220px 132px;
}
header .header_hidden .btn_serach {
  left: 0;
}
header .header_hidden .hidden_close {
  height: 44px;
  width: 44px;
  line-height: 44px;
  text-align: center;
  font-size: 16px;
  font-weight: 900;
  position: absolute;
  top: 0;
  right: 0;
}
header .header_hidden .hidden_close::after {
  background-position: -88px -44px;
}
header .header_hidden .hidden_nav {
  height: 44px;
  width: 100%;
  box-sizing: border-box;
  padding: 0px 44px;
}
header .header_hidden .hidden_nav a {
  float: left;
  display: block;
  width: 33.3%;
  height: 44px;
  line-height: 44px;
  text-align: center;
  font-size: 16px;
  font-weight: 900;
}

.goods_serach {
  border-bottom: 1px solid #e6e6e6;
  padding: 10px;
  position: relative;
  background-color: White;
}
.goods_serach input {
  font-size: 16px;
  height: 30px;
  line-height: 30px;
  border-radius: 15px;
}
.goods_serach .serachText {
  background: #f1f1f1;
  padding-left: 10px;
  width: 90%;
  padding-left: 5%;
  border: none;
}
.goods_serach .serachBtn {
  width: 63px;
  background: #ff841d;
  color: #FFFFFF;
  position: absolute;
  top: 10px;
  right: 10px;
}

.goods_nav {
  background-color: #fff;
  padding-top: 22px;
  padding-bottom: 6px;
  overflow: hidden;
}
.goods_nav .nav_item {
  float: left;
  width: 25%;
  margin-bottom: 16px;
  text-align: center;
}
.goods_nav .nav_item a {
  display: block;
}
.goods_nav .nav_item a .nav_pic img {
  width: 50px;
}
.goods_nav .nav_item:nth-of-type(n+9) {
  display: none;
}

.discount_push {
  background-color: #fff;
}
.discount_push .discount_title {
  height: 36px;
  margin-top: 4px;
}
.discount_push .discount_title a {
  display: block;
  height: 35px;
  background-color: #ff9000;
  background: linear-gradient(top, #ffaf47, #ff9000);
  border-bottom: solid 1px #ffc881;
  line-height: 35px;
  overflow: hidden;
  color: #fff;
  padding-left: 10px;
}
.discount_push .discount_title a h4 {
  line-height: 35px;
  font-weight: bold;
  font-size: 18px;
}
.discount_push .discount_title a img {
  height: 20px;
  width: 20px;
  margin-right: 10px;
  color: #FFF;
  margin-top: 8px;
}
.discount_push .discount_con ul li {
  padding: 14px 8px;
  position: relative;
}
.discount_push .discount_con ul li a {
  height: 100px;
  display: block;
}
.discount_push .discount_con ul li a .con_left {
  float: left;
  width: 110px;
}
.discount_push .discount_con ul li a .con_left img {
  width: 100px;
  height: 100px;
}
.discount_push .discount_con ul li a .con_right {
  padding-left: 90px;
  line-height: 24px;
}
.discount_push .discount_con ul li a .con_right .con_title {
  color: #333;
  word-break: break-all;
  font-size: 15px;
  line-height: 19px;
  min-height: 57px;
  text-align: left;
  font-weight: normal;
}
.discount_push .discount_con ul li a .con_right .con_title span {
  color: #ff6c00;
  display: inline;
}
.discount_push .discount_con ul li a .con_right .con_det span {
  color: #a1a1a1;
  font-size: 12px;
}
.discount_push .discount_con ul li a .con_right .con_det span i {
  background-image: url("../../images/2.svg");
  width: 14px;
  height: 15px;
  display: inline-block;
  background-size: auto 100%;
  background-repeat: no-repeat;
  background-position: center;
  vertical-align: middle;
  margin-right: 4px;
}

.coupon_mess {
  width: 100%;
  text-align: center;
  height: 52px;
  border: 1px solid transparent;
  background: #F4F4F4;
}
.coupon_mess .coupon_link {
  display: block;
  line-height: 30px;
  background: #F4F4F4;
  text-decoration: none;
  font-size: 13px;
  background-image: linear-gradient(#ffffff, #f4f4f4);
  margin: 10px;
  border: 1px solid #dddddd;
  border-radius: 4px;
}

.brand_rank {
  height: 36px;
  margin-top: 10px;
  margin-bottom: 4px;
  background: #F4F4F4;
}
.brand_rank a {
  display: block;
  height: 35px;
  background-color: #ff9000;
  background: linear-gradient(top, #ffaf47, #ff9000);
  border-bottom: solid 1px #ffc881;
  line-height: 35px;
  overflow: hidden;
  color: #fff;
  padding-left: 10px;
}
.brand_rank a h4 {
  line-height: 35px;
  font-weight: bold;
  font-size: 18px;
}
.brand_rank a img {
  height: 20px;
  width: 20px;
  margin-right: 10px;
  color: #FFF;
  margin-top: 8px;
}

footer .footer_top {
  margin-top: 10px;
  background: #efefef;
  border-top: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
  height: 34px;
  line-height: 34px;
}
footer .footer_top .footer_log {
  width: 30%;
}
footer .footer_top .footer_back {
  width: 40%;
  border: 0;
}
footer .footer_top a {
  display: block;
  height: 100%;
  text-align: center;
  box-sizing: border-box;
  float: left;
  font-size: 14px;
  border-right: 1px solid #c4c4c4;
}
footer .footer_top a img {
  width: 15px;
  height: 15px;
  vertical-align: middle;
  margin-top: -5px;
  margin-right: 2px;
}
footer .footer_bottom {
  text-align: center;
  background: #efefef;
  font-size: 12px;
  padding: 15px 0;
  color: #666;
}
footer .footer_bottom p a:first-child {
  color: red;
}

.app_link {
  position: fixed;
  bottom: 30px;
  right: 0;
  z-index: 76;
}
.app_link img {
  width: 60px;
}
.app_link .app_close {
  display: block;
  position: absolute;
  bottom: 50px;
  right: 50px;
  width: 25px;
  height: 25px;
  color: #eee;
  background-color: #000;
  background: url("../../images/icon_bar.png") no-repeat -362px -32px;
}
