  </div>
  <!--商品列表-->
  <div id="goods">
    <div class="container">
      <ul>
      </ul>
    </div>
  </div>
  <!--分页栏-->
  <div id="paging">
    <div class="up"><button>上一页</button></div>
    <div>
      <select id="selectPage">
        
