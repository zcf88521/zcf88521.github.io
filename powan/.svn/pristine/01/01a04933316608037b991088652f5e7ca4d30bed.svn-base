    <div class="footer-bottom">
      <p>
        <a href="JavaScript:void(0)">手机APP下载</a>
        <a href="JavaScript:void(0)">慢慢买手机版</a> -- 掌上比价平台
      </p>
      <p>m.manmanbuy.com</p>
    </div>
  </div>
</body>

</html>
<!--JQ-->
<script src="../../js/jquery-1.12.2.js"></script>
<!--bootstarp-->
<script src="../../js/bootstrap.js"></script>
<!--模板引擎-->
<script src="../../js/template.js"></script>
<!--触摸点击封装方法-->
<script src="../../js/touch_click.js"></script>
<!--商品列表模版-->
<script id="overseas" type="text/html">
  {{each result as value index}}
  <li>
    <a href="JavaScript:void(0)">
      <div class="media">
        <div class="media-left">
          {{#value.productImg2}}
        </div>
        <div class="media-body">
          <h4 class="media-heading">{{value.productName}}<span>{{value.productPinkage}}</span></h4>
          <div class="other">
            <span class="f_l">{{value.productFrom}}</span>
            <span class="f_r">
                    <i></i>
                    {{value.productComCount}}
                  </span>
          </div>
        </div>
      </div>
    </a>
  </li>
  {{/each}}
</script>
<script src="../../js/ljj/overseas.js"></script>