#header {
  height: 44px;
  background-color: #ff841d;
  width: 100%;
  width: 100%;
  position: relative;
  padding: 0 50px;
  text-align: center;
  line-height: 44px; }
  #header a {
    position: absolute;
    height: 100%;
    width: 50px;
    color: #fff;
    font-size: 16px; }
    #header a:nth-of-type(1) {
      left: 0;
      top: 0;
      background: url("../../images/arr_left.png") no-repeat center; }
    #header a:nth-of-type(2) {
      top: 0;
      right: 0;
      background: url("../../images/header_app.png") no-repeat center/32px; }
  #header h1 {
    font-size: 20px;
    font-weight: 900;
    color: #fff;
    line-height: 44px; }

#directory {
  height: 36px;
  border-top: 1px solid #ebebeb;
  border-bottom: 1px solid #ccc;
  line-height: 36px;
  padding-left: 8px;
  font-size: 14px;
  background: -webkit-linear-gradient(top, #ffffff, #ebebeb);
  background: linear-gradient(top, #ffffff, #ebebeb); }
  #directory a {
    color: #000;
    text-decoration: none; }

#goods {
  padding: 0; }
  #goods .row {
    margin: 0;
    padding-bottom: 10px; }
    #goods .row li {
      padding: 10px 0 0 10px;
      border-bottom: 1px solid #eee;
      border-right: 1px solid #eee; }
      #goods .row li > a {
        text-decoration: none;
        display: block; }
        #goods .row li > a .img {
          margin: 10px auto;
          overflow: hidden;
          width: 140px;
          height: 140px; }
          #goods .row li > a .img img {
            width: 100%;
            height: 100%; }
        #goods .row li > a .title {
          color: #333333;
          font-size: 14px;
          line-height: 18px;
          height: 36px;
          padding: 0 8px;
          overflow: hidden;
          -webkit-line-clamp: 1;
          -webkit-box-orient: vertical;
          display: -webkit-box; }
        #goods .row li > a .money {
          color: #ff6c00;
          margin-top: 4px;
          font-size: 14px;
          padding: 0 8px;
          height: 18px;
          line-height: 18px;
          -webkit-line-clamp: 1;
          -webkit-box-orient: vertical;
          display: -webkit-box;
          overflow: hidden; }
        #goods .row li > a .info {
          padding: 6px 8px;
          color: #a1a1a1;
          font-size: 12px; }
      #goods .row li:nth-child(2n) {
        border-right: 1px solid transparent; }

.lodding {
  margin-top: 15px;
  text-align: center;
  background-color: #f8f8f8; }
  .lodding img {
    width: 32px;
    height: 32px;
    vertical-align: middle; }

.footer {
  background-color: #efefef;
  margin-top: 25px; }
  .footer .footer-top {
    border-top: 1px solid #c4c4c4;
    border-bottom: 1px solid #c4c4c4; }
    .footer .footer-top a {
      border-right: 1px solid #c4c4c4;
      text-align: center;
      height: 34px;
      line-height: 34px;
      float: left;
      font-size: 14px;
      width: 30%;
      color: #000; }
      .footer .footer-top a img {
        width: 15px;
        height: 15px;
        vertical-align: middle;
        -webkit-transform: translate(-5px, -2px);
            -ms-transform: translate(-5px, -2px);
                transform: translate(-5px, -2px); }
  .footer .footer-bottom {
    padding: 15px 0; }
    .footer .footer-bottom p {
      text-align: center;
      font-size: 12px;
      color: #666; }
      .footer .footer-bottom p a:nth-child(1) {
        color: red; }
      .footer .footer-bottom p a:nth-child(2) {
        color: #000; }
