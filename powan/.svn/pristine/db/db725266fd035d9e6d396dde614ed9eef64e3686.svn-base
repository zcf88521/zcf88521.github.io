@import '../../css/bootstrap.css';
@import '../../css/normalize.css';
@import '../../css/base.css';


.html,body,p{
    padding: 0;
    margin: 0;
}
body{
    background-color: #f5f9f3;
}
// 白菜价顶部
.cabbage_heard{
    width: 100%;
    height: 44px;
    background:#e4393c;
    text-align: center;
    line-height: 44px;
    font-size: 20px;
    font-weight: 800;
    color: white;
    padding-left: 20px;
    padding-right: 50px;
        a{
            color: white;
            position: absolute;
            top: 11px;
            &.cabbage_heard_left{
                font-size: 30px;
                left:20px;
                top:6px;
            }
            &.cabbage_heard_right{
                right: 10px;
                background-image: url("../../img/header_app.png");
                background-repeat: no-repeat;
                background-size:32px 32px;
                width:32px;
                height:44px;
                top:6px;
            }
        }
     }

    //  白菜价导航
    .cabbage_nav{
        background-color:white;
        width: 100%;
        height: 38px;
        border-bottom: 1px solid #efefef;
        position: relative;
        padding-right:38px;
        overflow: hidden;
        .cabbage_nav_all{
            ul{
                width: 1000%;
                height: 38px;
                line-height: 38px;
                li{                 
                    float: left;
                    padding: 0 13px;
                    a{
                         height:36px;
                         text-align: center;
                         line-height:36px;
                         font-size: 14px;
                         color: black;
                         display: block;                    
                    }
                }
            }
        }
        span{
            position: absolute;
            right: 0;
            width: 38px;
            height: 38px;
            font-size:25px;
            line-height: 38px;
            text-align: center;
            color: gray;
            border-left: 1px solid #efefef;
            background-color: white;
            z-index: 1;
        }

    }

    //  白菜价img
    .cabbage_img{
        text-align: center;
        margin-top: 10px;
        margin-bottom: 10px;
        width: 100%;
        img{
            width: 100%;
        }
    }

    // 白菜价 内容
    .cabbage_content{
        width: 100%;
        height:152px;
        margin: 6px auto;
        background-color: #ffffff;
        position: relative;
        padding: 10px 5px;
        box-sizing: border-box;
        overflow: hidden;
        border-top: 1px solid #e1e1e1;
        border-bottom: 1px solid #e1e1e1;  
    }

    // 白菜价底部
    .cabbage_endback{
        width: 100%;
        margin-top: 20px;
        height: 38px;
        border-top: solid 1px #ebebeb;
        border-bottom: 1px solid #ccc;
        line-height: 36px;
         padding-left: 8px;
         background: -webkit-gradient(linear,0 0,0 100%,from(#ffffff),to(#ebebeb));
         a{
             font-size: 14px;
             color: black;
              font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif !important;        
             &:hover{
                  text-decoration: none;
                  color: black;
             }
         }
    }

    // 白菜价link
    .cabbage_link{
        width: 100%;
        height:100px;
        background-color: #434343;
        margin-top: 20px;
        padding: 10px;
        position: relative;
        .media-left{
            a{
            img{
                width: 64px;
                height: 64px;
            }
        }
        } 
        .media-body{
            color:white;
            a{
                display: block;
                width: 80px;
                height: 30px;
                line-height: 30px;
                font-size: 14px;
                color: #FFFFFF;
                background: #6db110;
                border-radius: 4px;
                text-align: center;
                position: absolute;
                top: 4px;
                right: 10px;
            }
        }
    }

    // 白菜价底部
    .cabbage_footer{
        background: #efefef;
        text-align: center;
        font-size: 12px;
        padding: 15px 0;
        color: #666;
        width: 100%;
        // height: 58px;
        line-height: 10px;
        border-top: 1px solid #e1e1e1;
        border-bottom: 1px solid #e1e1e1;
        margin-top: 20px;
        .foot_one{
            color: red;
        }
        .foot_two{
            color:#666;
        }
    }