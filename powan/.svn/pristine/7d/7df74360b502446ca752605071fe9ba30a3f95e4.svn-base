<!--模板引擎-->
<script src="js/template.js"></script>
<!--商品列表模板-->
<script id="listing" type="text/html">
  {{each result as value index}}
  <li class="col-xs-6">
    <a href="inlanddiscount_detail.html?productid={{value.productId}}">
      <div class="img">{{#value.productImg}}</div>
      <p class="title">{{value.productName}}</p>
      <p class="money">{{value.productPrice}}</p>
      <div class="info">{{value.productFrom}} | {{value.productTime}}</div>
    </a>
  </li>
  {{/each}}
</script>
<!--商品加载模版-->
<script id="to-lodding" type="text/html">
  <li class="col-xs-6">
    <a href="inlanddiscount_detailetail.html?productid={{productId}}">
      <div class="img">{{#productImg}}</div>
      <p class="title">{{productName}}</p>
      <p class="money">{{productPrice}}</p>
      <div class="info">{{productFrom}} | {{productTime}}</div>
    </a>
  </li>
</script>
