html,
body {
  padding: 0;
  margin: 0;
}

body {
  min-width: 320px;
  background-color: #f5f9f3;
  transition: none;
}

.kfc_hearder {
  background: #f4483e;
  color: #FFFFFF;
  height: 44px;
  width: 100%;
  position: relative;
}
.kfc_hearder .kfc_back {
  position: absolute;
  width: 50px;
  height: 44px;
  text-align: center;
  left: 0;
}
.kfc_hearder h1 {
  display: block;
  margin: 0;
  text-align: center;
  line-height: 44px;
  font-size: 20px;
  font-weight: bold;
}
.kfc_hearder .kfc_right {
  position: absolute;
  width: 50px;
  height: 44px;
  line-height: 44px;
  right: 5px;
  top: 0px;
}
.kfc_hearder .kfc_right img {
  width: 80%;
  height: 80%;
}

.kfc_info {
  font-size: 12px;
  text-align: center;
  color: #999;
  margin: 8px 0px;
}

.kfc_contant {
  width: 100%;
  height: 100%;
}
.kfc_contant .media {
  border-bottom: 1px solid #f1f1f1;
}
.kfc_contant .media img {
  width: 100px;
  height: 80px;
}
.kfc_contant .name {
  font-size: 14px;
}
.kfc_contant .kfc_time {
  font-size: 14px;
  color: #EE7700;
  margin-top: 10px;
}
.kfc_contant li {
  margin-top: 10px;
}
