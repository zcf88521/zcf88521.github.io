#header {
  height: 44px;
  background-color: #ff841d;
  width: 100%;
  width: 100%;
  position: relative;
  padding: 0 50px;
  text-align: center;
  line-height: 44px; }
  #header a {
    position: absolute;
    height: 100%;
    width: 50px;
    color: #fff;
    font-size: 16px; }
    #header a:nth-of-type(1) {
      left: 0;
      top: 0; }
    #header a:nth-of-type(2) {
      top: 0;
      right: 0;
      background: url("../../images/header_app.png") no-repeat center/32px; }
  #header h1 {
    font-size: 20px;
    font-weight: 900;
    color: #fff;
    line-height: 44px; }

#goods .container {
  padding: 0; }
  #goods .container ul li {
    padding: 14px 8px;
    position: relative;
    border-bottom: 1px solid #e0e0e0; }
    #goods .container ul li a {
      text-decoration: none;
      display: block; }
      #goods .container ul li a .media-left {
        padding: 0; }
        #goods .container ul li a .media-left img {
          width: 100px;
          display: block;
          vertical-align: middle; }
      #goods .container ul li a .media-body h4 {
        color: #333;
        font-size: 15px;
        font-weight: 500;
        min-height: 57px;
        margin: 0; }
        #goods .container ul li a .media-body h4 span {
          color: #ff6c00; }
      #goods .container ul li a .media-body .other {
        margin-top: 8px;
        color: #a1a1a1;
        line-height: 1; }
        #goods .container ul li a .media-body .other i {
          margin-right: 2px;
          width: 15px;
          height: 15px;
          background: url("../../images/comment.png") no-repeat center/15px 11px;
          float: left; }

#directory {
  height: 36px;
  border-top: 1px solid #ebebeb;
  border-bottom: 1px solid #ccc;
  line-height: 36px;
  padding-left: 8px;
  font-size: 14px;
  background: -webkit-linear-gradient(top, #ffffff, #ebebeb);
  background: linear-gradient(top, #ffffff, #ebebeb); }
  #directory a {
    color: #000;
    text-decoration: none; }

.footer {
  background-color: #efefef;
  margin-top: 25px; }
  .footer .footer-top {
    border-top: 1px solid #c4c4c4;
    border-bottom: 1px solid #c4c4c4; }
    .footer .footer-top a {
      border-right: 1px solid #c4c4c4;
      text-align: center;
      height: 34px;
      line-height: 34px;
      float: left;
      font-size: 14px;
      width: 30%;
      color: #000; }
      .footer .footer-top a img {
        width: 15px;
        height: 15px;
        vertical-align: middle;
        -webkit-transform: translate(-5px, -2px);
            -ms-transform: translate(-5px, -2px);
                transform: translate(-5px, -2px); }
  .footer .footer-bottom {
    padding: 15px 0; }
    .footer .footer-bottom p {
      text-align: center;
      font-size: 12px;
      color: #666; }
      .footer .footer-bottom p a:nth-child(1) {
        color: red; }
      .footer .footer-bottom p a:nth-child(2) {
        color: #000; }
