body {
  background: #f3f3f3;
}

.appris_nav {
  height: 36px;
  background-color: #ebebeb;
  border-top: 1px solid #ebebeb;
  border-bottom: 1px solid #ebebeb;
  background: -webkit-linear-gradient(top, #fff, #ebebeb);
  background: linear-gradient(top, #fff, #ebebeb);
  text-align: left;
  line-height: 36px;
  font-size: .7em;
}
.appris_nav span:nth-of-type(1) {
  color: #666;
  padding-left: 5px;
}

.appris_mian .hd {
  background: #f3f3f3;
  color: #874604;
}
.appris_mian .hd h3 {
  line-height: 36px;
  border-top: 1px solid #c8c8c8;
  border-bottom: 1px solid #c8c8c8;
  font-size: .6em;
  font-weight: 600;
  padding-left: 5px;
}
.appris_mian .body ul {
  padding-bottom: 30px;
}
.appris_mian .body ul li {
  padding: 10px;
  border-bottom: 1px solid #e7e7e7;
}
.appris_mian .body ul li a {
  display: block;
}
.appris_mian .body ul li a .protit_img img {
  width: 80px;
  height: 80px;
}
.appris_mian .body ul li a .tit {
  font-size: .5em;
  overflow: hidden;
  padding-left: 10px;
  padding-top: 5px;
  color: #888;
}
.appris_mian .body ul li .pro_appris {
  background: #f8f8f8;
  margin-top: 10px;
  font-size: .7em;
  padding: 10px;
  border: 1px solid #e7e7e7;
  position: relative;
}
.appris_mian .body ul li .pro_appris .arrow {
  width: 0;
  height: 0;
  position: absolute;
  display: block;
  left: 29px;
  top: -8px;
  border-bottom: 8px solid #f8f8f8;
  border-left: 8px solid transparent;
  border-right: 8px solid transparent;
}
.appris_mian .body ul li .pro_appris .pro_app_hd {
  height: 20px;
  line-height: 20px;
}
.appris_mian .body ul li .pro_appris .star5 {
  display: block;
  height: 18px;
  width: 90px;
  background: url(../../images/star.svg) repeat-x center/18px 18px;
}
.appris_mian .body ul li .pro_appris p {
  padding-top: 10px;
  background: #f8f8f8;
}
