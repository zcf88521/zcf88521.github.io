@import '../../css/bootstrap.css';
@import '../../css/normalize.css';
@import '../../css/base.css';


.html,body,p{
    padding: 0;
    margin: 0;
}
body{
    min-width: 320px;
    background-color: #f5f9f3;
    transition: none;
}
.bt{
    color: red;
    border-bottom: 2px solid red;
}
 
// 白菜价顶部
.cabbage_heard{
    background: #f4483e;
    color: #FFFFFF;
    height: 44px;
    width: 100%;
    position: relative;
        .cabbage_heard_left{
             position: absolute;
             width: 50px;
             height: 44px;
            text-align: center;
            left: 0;    
        }
         h1{
             display: block;         
            margin: 0;
            text-align: center;
            line-height: 44px;
            font-size: 20px;
            font-weight: bold;
        }
        .cabbage_heard_right{
            position: absolute;
            width: 50px;
           height: 44px;
           line-height: 44px;
          right: 5px;
          top: 0px;
        img{
            width: 80%;
            height: 80%;
        }  
        }
}


    //  白菜价导航
    .cabbage_nav{
        background-color:white;
        width: 100%;
        height: 38px;
        border-bottom: 1px solid #efefef;
        position: relative;   
        padding-right:38px;  
        overflow: hidden;      
        .cabbage_nav_all{
           ul{
                  width: 900px;
                  height: 38px;
                  line-height: 38px;                  
               
                li{                 
                    float: left;
                    padding: 0 13px;
                    a{
                         height:36px;
                         text-align: center;
                         line-height:36px;
                         font-size: 17px;
                         color: #333;
                         display: block;  
                         text-decoration: none;      
                    }
                }
            }
        }
        .show{
            width: 38px;
            height: 38px;
            position: absolute;
            right: 0;
            font-size: 20px;
            text-align: center;
            line-height: 38px;
            background-color: #fff;
            border-left: 1px solid #c1c1c1;
            color: gray;
        }             
    }

     .seachBar{
            position:absolute;
            top: 83px;
            left: 0px;
            z-index:10;
            width: 320px;
            height: 55px;                     
            display: none;        
            box-shadow: 0px 3px 6px #dddddd;
            background-color:#f7f7f7;  
             padding:10px 0;          
            .txt{
                padding-left: 10px;   
                width: 262px;       
                height: 35px;               
                    span{
                        position: absolute;                    
                        top:22px;
                        left:16px;;
                        z-index: 2;
                        color: #ccc
                    }
                     input{
                         width: 100%;                        
                         height: 100%;
                         padding-left: 26px;
                     }
            }
            .sub{
                height: 35px;
                width: 40px;
                margin-left: 10px;
                input{
                    width: 100%;
                    height: 100%;
                    background-color: #f7f7f7;
                }                
            }    
        }  

    //  白菜价img
    .cabbage_img{
        text-align: center;
        margin-top: 10px;
        margin-bottom: 10px;
        width: 100%;
        img{
            width: 100%;
        }
    }

    // 白菜价 内容
    .cabbage_content{
        width: 100%;
        height:100%;
        background-color: #ffffff;
        position: relative;
        li{
           padding: 10px 5px;
           box-sizing: border-box;
           margin: 6px auto;
           overflow: hidden;
           border-top: 1px solid #e1e1e1;
           border-bottom: 1px solid #e1e1e1;
        }
            .media{
                img{
                    width: 130px;
                    height: 130px;
                }
            }
            .media-body{
                .media-heading{
                    min-height: 28px;
                    max-height: 40px;
                    font-size: 14px;
                }
                .content_pr{
                    font-size: 24px;
                    color: #fc353a;
                    margin-top: 4px;                  
                    width: 100%;
                    height: auto;
                    overflow: visible;
                    i{
                        font-size: 16px;
                        vertical-align: middle;
                        font-style: normal;
                        margin-right: 4px;
                    }
                    em{
                        font-size: 12px;
                        vertical-align: middle;
                        font-style: normal;
                    }
                    del{
                        text-decoration: line-through;
                        font-size: 13px;
                        color: #a8a8a8;
                        margin-left: 12px;
                        vertical-align: middle;
                    }
                }
                .bar{
                   i{
                        position: relative;
                        box-sizing: border-box;
                        width: 82px;
                        height: 14px;
                        border: 1px solid #fc353a;
                        border-radius: 6px;
                        color: #fff;
                        font-weight: 900;
                        background: #ffb7b7;
                        text-align: center;
                        overflow: hidden;
                        display: inline-block;
                        em{
                            position: absolute;
                            top: 0;
                            left: 0;
                            height: 100%;
                            border-radius: 5px;
                            background: #fc353a;
                            display: block;
                            transition: all 0.3s                                   cubic-bezier(0.39, 0.58, 0.57, 1);
                        }
                        span{
                            font-size: 12px;
                            line-height: 11px;
                            display: block;
                            width: 100%;
                            height: 100%;
                            text-align: center;
                            position: absolute;
                            top: 0px;
                            left: 0px;
                        }
                   }
                   b{
                        font-weight: normal;
                        font-size: 12px;
                        color: #999999;
                        vertical-align: 3px;
                        margin-left: 10px;
                        color: #fc353a;
                   }
                }  
                .b{
                    margin-top: 3px;
                    position: relative;
                    .q{     
                        
                        top: 6px;
                        left: 135px;
                        float: left;
                         a{
                            border: 1px dotted #e4393c;
                            height: 26px;
                            line-height: 26px;
                            text-align: center;
                            display: block;
                            padding: 0px 5px;
                            font-size: 12px;
                            color: #e4393c;
                            position: absolute;                
                        }                              
                    }  
                    .g{
                        a{
                            height: 38px;
                            line-height: 38px;
                            text-align: center;
                            display: block;
                            background-color: #ff5e5d;
                            color: #ffffff;
                            border-radius: 4px;
                            padding: 0px 3px;
                            font-size: 12px;                                            
                        }
                    }                 
                }                          
            }
    }

    // 白菜价底部
    .cabbage_endback{
        width: 100%;
        margin-top: 20px;
        height: 38px;
        border-top: solid 1px #ebebeb;
        border-bottom: 1px solid #ccc;
        line-height: 36px;
         padding-left: 8px;
         background: gradient(linear,0 0,0 100%,from(#ffffff),to(#ebebeb));
         a{
             font-size: 14px;
             color: black;
              font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif !important;        
             &:hover{
                  text-decoration: none;
                  color: black;
             }
         }
    }

    // 白菜价link
    .cabbage_link{
        width: 100%;
        height:100px;
        background-color: #434343;
        margin-top: 20px;
        padding: 10px;
        position: relative;
        .media-left{
            a{
            img{
                width: 64px;
                height: 64px;
            }
        }
        } 
        .media-body{
            color:white;
            a{
                display: block;
                width: 80px;
                height: 30px;
                line-height: 30px;
                font-size: 14px;
                color: #FFFFFF;
                background: #6db110;
                border-radius: 4px;
                text-align: center;
                position: absolute;
                top: 4px;
                right: 10px;
            }
        }
    }

    // 白菜价底部
    .cabbage_footer{
        background: #efefef;
        text-align: center;
        font-size: 12px;
        padding: 15px 0;
        color: #666;
        width: 100%;
        // height: 58px;
        line-height: 10px;
        border-top: 1px solid #e1e1e1;
        border-bottom: 1px solid #e1e1e1;
        margin-top: 20px;
        .foot_one{
            color: red;
        }
        .foot_two{
            color:#666;
        }
    }

    // 返回顶部
    .cabbage_backtop{
            display: none;
        
        a{
            position: fixed;
            z-index: 999;
            bottom: 30px;
            right: 8px;
            width: 40px;
            text-align: center;
            line-height: 40px;
            font-size: 30px;
            height: 40px;
            border-radius: 20px;
            opacity: 0.5;
        }
    }