header {
  height: 51px;
  background-color: #ff841d;
  border-bottom: 1px solid #e6e6e6;
}
header a {
  display: block;
  height: 100%;
}
header a img {
  overflow: hidden;
}
header .header_left {
  width: 75%;
  margin-left: 5px;
  padding: 12px;
}
header .header_left img {
  max-height: 25px;
}
header .header_right {
  padding: 7px;
}
header .header_right img {
  max-height: 34px;
}
header .header_hidden {
  background: #EFEFEF;
  border-bottom: 1px solid #ddd;
  height: 44px;
  position: fixed;
  display: none;
}
header .header_hidden .btn_serach {
  height: 44px;
  width: 44px;
  overflow: hidden;
  position: relative;
  color: transparent;
}

.goods_serach {
  border-bottom: 1px solid #e6e6e6;
  padding: 10px;
  position: relative;
  background-color: White;
}
.goods_serach input {
  font-size: 16px;
  height: 30px;
  line-height: 30px;
  border-radius: 15px;
}
.goods_serach .serachText {
  background: #f1f1f1;
  padding-left: 10px;
  width: 90%;
  padding-left: 5%;
  border: none;
}
.goods_serach .serachBtn {
  width: 63px;
  background: #ff841d;
  color: #FFFFFF;
  position: absolute;
  top: 10px;
  right: 10px;
}
