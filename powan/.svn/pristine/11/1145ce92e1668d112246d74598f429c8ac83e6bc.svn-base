<script id="template" type="text/html">
  {{each result as value index}}
  <div class="nav_item">
    <a href="{{value.titlehref}}">
      <div class="nav_pic">
        {{#value.img}}
      </div>
      <div class="nav_name">
        {{value.name}}
      </div>
    </a>
  </div>
  {{/each}}
</script>
<script id="template2" type="text/html">
