header{
  display: flex !important;
  background:#ff841d;
  justify-content: space-between;
  align-items: center;
  height: 44px;
  a{
      display: block;
      width: 50px;
      height: 44px;
  }
  .back{
    flex: 1;
    a{
      background: url(../../images/arr_left.png) no-repeat center;
    }
  }
  h1{
    flex: 6;
    display: block;
      line-height: 44px;
      text-align: center;
      font-size: 1.1em;
      color: white;
      font-weight: 600;
      margin: 0;
    }
   .appDown{
     flex: 1;
     position: relative;
      a{
        background: url(../../images/header_app.png)no-repeat center/ 35px 35px;
      }
    }
}