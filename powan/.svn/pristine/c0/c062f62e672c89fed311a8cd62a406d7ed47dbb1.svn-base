                <a href="#" class="footer_back"><img src="../../images/returnTop.png" alt="">返回顶部</a>
            </div>
            <div class="footer_bottom">
                <p>
                    <a href="http://m.manmanbuy.com/?o=download">手机APP下载</a> <a href="http://m.manmanbuy.com">
                    慢慢买手机版</a> -- 掌上比价平台</p>
                <p>
                    m.manmanbuy.com
                </p>
            </div>
        </footer>
</body>

</html>
<!--使用模板引擎=======================================-->
<script id="oneProduct" type="text/html">
    {{each result as value index}}
    <h4>{{value.productName}}</h4>
    <div class="oneProduct-top">{{#value.productImg}}</div>
    <!--使用bootstrap模板页面切换-->
    <div class="product-tab">
        <!-- Nav tabs -->
        <ul class="nav nav-tabs" role="tablist">
            <li role="presentation" class="active product-width"><a href="#home" class="my_active" aria-controls="home" role="tab" data-toggle="tab">比价购买</a></li>
            <li role="presentation" class="product-width "><a href="#profile" aria-controls="profile" role="tab" data-toggle="tab">产品参数</a></li>
            <li role="presentation" class="product-width"><a href="#messages" aria-controls="messages" role="tab" data-toggle="tab">评价(2322)</a></li>
        </ul>
        <!-- Tab panes -->
        <div class="tab-content">
            <div role="tabpanel" class="tab-pane active" id="home">
                <div class="div">{{#value.bjShop}}</div>
            </div>
            <div role="tabpanel" class="tab-pane" id="profile">.2..</div>
            <div role="tabpanel" class="tab-pane" id="messages">
                <div class="productcom">
                    <ul class="productcom-ul">
                        <h5>网友评价</h5>
                        <!--这是一个坑-->
                    </ul>

                </div>
            </div>
        </div>
    </div>

    {{/each}}
</script>
<!--评论处使用模板引擎=======================================-->
<script id="productcom" type="text/html">
    {{each result as value index}}
    <li>

        <div class="comtop">
            <span class="lineFriend">{{value.comName}}</span>
            <span class="comdata">{{value.comTime}}</span>
        </div>
        <div class="buyFrom f_r">{{value.comFrom}}</div>
        <div class="combottom">{{value.comContent}}</div>
    </li>
    {{/each}}
</script>
<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
<script src="../../js/jquery-1.12.2.js"></script>
